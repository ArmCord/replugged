{
  "name": "replugged",
  "version": "4.0.0",
  "description": "A lightweight @discord client mod focused on simplicity and performance",
  "license": "MIT",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "parcel build",
    "watch": "parcel watch",
    "check": "tsc --noEmit",
    "lint": "eslint --ext js --ext ts --ext jsx --ext tsx .",
    "format": "npm run lint -- --fix"
  },
  "repository": "https://github.com/replugged-org/replugged.git",
  "author": "Replugged",
  "bugs": {
    "url": "https://github.com/replugged-org/replugged/issues"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "electron": "13",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "parcel": "^2.7.0",
    "typescript": "^4.8.4"
  },
  "targets": {
    "main": {
      "context": "electron-main",
      "source": "src/main/index.ts",
      "includeNodeModules": {
        "electron": false
      }
    },
    "preload": {
      "context": "electron-renderer",
      "source": "src/preload.ts",
      "includeNodeModules": {
        "electron": false
      }
    },
    "renderer": {
      "context": "browser",
      "source": "src/renderer/index.ts",
      "includeNodeModules": true
    }
  }
}
